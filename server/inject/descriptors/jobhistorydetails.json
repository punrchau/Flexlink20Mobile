        "store": {
          "type": "string"
        },
        "jobpriority": {
          "type": "string"
        },
        "warrantystatus": {
          "type": "string"
        },
        "warrantyexpiry": {
          "type": "string"
        },
        "imei": {
          "type": "string"
        },
        "productmodel": {
          "type": "string"
        },
        "unitcondition": {
          "type": "string"
        },
        "phonecarrier": {
          "type": "string"
        },
        "swapserialnumber": {
          "type": "string"
        }
