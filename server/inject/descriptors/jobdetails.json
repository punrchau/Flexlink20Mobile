{
  "type": "object",
  "comment": "Categories",
  "properties": {
    "categories": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "categoryName": {
            "type": "string"
          },
          "icon": {
            "type": "string",
            "selector": "#sjsPage > div.col-lg-9.col-md-9.col-sm-9.col-xs-9.text-right > button:nth-child(7)",
            "control": "property.innerText"
          },
          "image": {
            "type": "string"
          },
          "action": {
            "type": "action",
            "control": "action.link",
            "selectContains": "VIEW"
          }
        }
      },
      "selectorAll": ".col-lg-9.col-md-9.col-sm-9.col-xs-9.text-right"
    },
    "actions": {
      "type": "object",
      "properties": {
        "cancel": {
          "type": "action"
        },
        "viewjob": {
          "type": "action",
          "control": "action.button",
          "selector": "button[value=\"View\"]"
        }
      }
    },
    "jobinfo": {
      "type": "object",
      "properties": {
        "ticketnumber": {
          "type": "string"
        }
      }
    },
    "isjobdetails": {
      "type": "identifier",
      "selector": "h2",
      "control": "exists",
      "selectContains": "Service Jobs"
    }
  },
  "connect": []
}
