{
  "type": "object",
  "comment": "Categories",
  "properties": {
    "categories": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "categoryName": {
            "type": "string"
          },
          "icon": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "action": {
            "type": "action"
          }
        }
      }
    },
    "actions": {
      "type": "object",
      "properties": {
        "cancel": {
          "type": "action"
        },
        "jobinfo": {
          "type": "action",
          "selector": ".btn.btn-info.ng-scope",
          "control": "action.link",
          "selectContains": "VIEW"
        }
      }
    },
    "jobinfo": {
      "type": "object",
      "properties": {
        "ticketnumber": {
          "type": "string"
        }
      }
    },
    "isjobdetails": {
      "type": "identifier",
      "selector": "h2",
      "control": "property.innerText",
      "selectContains": "Service Jobs"
    }
  },
  "connect": []
}
