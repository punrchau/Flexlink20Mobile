{
  "type": "object",
  "comment": "ListView",
  "properties": {
    "listItems": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "action": {
            "type": "action",
            "selector": ".btn.btn-info.ng-scope",
            "control": "action.button"
          },
          "ticketnumber": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "control": "property.innerText"
          },
          "firstname": {
            "type": "string",
            "selector": "td",
            "selectIndex": 13,
            "control": "property.innerText"
          },
          "problemcategory": {
            "type": "string",
            "selector": "td",
            "selectIndex": 2,
            "control": "property.innerText"
          },
          "lastname": {
            "type": "string",
            "selector": "td",
            "selectIndex": 14,
            "control": "property.innerText"
          },
          "mobilenumber": {
            "type": "string",
            "selector": "td",
            "selectIndex": 15,
            "control": "property.innerText"
          },
          "pickupaddress": {
            "type": "string",
            "selector": "td",
            "selectIndex": 16,
            "control": "property.innerText"
          },
          "selectRecord": {
            "type": "action",
            "selector": ".",
            "control": "action.link"
          },
          "isimp": {
            "type": "string",
            "control": "property.innerText",
            "selector": "td",
            "selectIndex": 17
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": "#gdServiceJobs  table tbody tr"
    },
    "selectTabs": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "tabName": {
            "type": "string",
            "selector": "a",
            "control": "property.innerText"
          },
          "isSelected": {
            "type": "boolean",
            "control": "exists",
            "selector": {
              "fn": "function(node){if(node.getAttribute('class')=='active'){return true}}",
              "selector": ""
            }
          },
          "clickTab": {
            "type": "action",
            "control": "action.link",
            "selector": "a"
          }
        }
      },
      "selectorAll": "#stateTabs li[class=\"active\"]"
    },
    "isrepairjob": {
      "type": "identifier",
      "selector": "#sjsPage > div.col-lg-3.col-md-3.col-sm-3.col-xs-3.page-heading > h2",
      "control": "exists"
    },
    "viewRecord": {
      "type": "action",
      "selector": ".btn.btn-info.ng-scope",
      "control": "action.button",
      "selectContains": "EDIT"
    },
    "searchstring": {
      "type": "string"
    },
    "search": {
      "type": "action"
    },
    "actions": {
      "type": "object",
      "properties": {
        "unitinfo": {
          "type": "action",
          "selector": "#sjsPage > div.col-lg-9.col-md-9.col-sm-9.col-xs-9.text-right > button:nth-child(5)",
          "control": "action.button"
        },
        "viewjobhistory": {
          "type": "action",
          "selector": "#sjsPage > div.col-lg-9.col-md-9.col-sm-9.col-xs-9.text-right > button:nth-child(4)",
          "control": "action.button"
        },
        "jobinfo": {
          "type": "action",
          "selector": "button[value=\"View\"]",
          "control": "action.button"
        },
        "ownerinfo": {
          "type": "action",
          "selector": "#sjsPage > div.col-lg-9.col-md-9.col-sm-9.col-xs-9.text-right > button:nth-child(3)",
          "control": "action.button"
        },
        "servicejobremarks": {
          "type": "action",
          "selector": "#sjsPage > div.col-lg-9.col-md-9.col-sm-9.col-xs-9.text-right > button:nth-child(10)",
          "control": "action.button"
        },
        "photoupload": {
          "type": "action"
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isrepairjob"
      ],
      "screens": [
        "servicejob"
      ]
    }
  ],
  "selector": ".row.margin-body"
}
