{
  "type": "object",
  "comment": "UserProfile",
  "properties": {
    "upload": {
      "type": "action",
      "selector": ".btn.btn-primary.hideFileButton",
      "control": "action.button"
    },
    "isunitphoto": {
      "type": "identifier",
      "selector": "#PhotoUploadModel > div > div > div.modal-header > h4",
      "control": "exists",
      "selectContains": "Unit Photo"
    },
    "cancel": {
      "type": "action",
      "selector": "#PhotoUploadModel > div > div > div.modal-header > button",
      "control": "action.button"
    }
  },
  "connect": [
    {
      "match": [],
      "event": "InvokePhotoUpload"
    },
    {
      "match": [
        "isunitphoto"
      ],
      "action": "upload"
    }
  ],
  "selector": "#PhotoUploadModel"
}
